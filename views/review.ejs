
<%if (auth=='600c0f0fbc4b391f0135065e') { %>
    <div class="editPost">
        <a href="/get/video/<%= review.postType %>/<%= review.slug %>/<%= review._id %>/" ><i class="fa fa-pencil-square-o"></i></a>
        <a href="/delete/video/<%= review.postType %>/<%= review.slug %>/<%= review._id %>/" onclick="return confirm('Are you sure you want to delete this post?');"><i class="fa fa-trash"></i></a>
    </div>
    <br><br>
<% } %>

<div id="fh5co-single-content" class="container-fluid pb-4 pt-4 paddding">
    <div class="container paddding">
        <%if (success.length > 0) { %>
            <div class="alert alert-success alert-dismissible message" style="text-align: center;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Congratulations!</strong> <%- success %>
            </div>
        <% } else if (warning.length > 0) { %>
            <div class="alert alert-warning alert-dismissible message" style="text-align: center;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Warning!</strong> <%- warning %>
            </div>
        <% } else if (danger.length > 0) { %>
            <div class="alert alert-danger alert-dismissible message">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Danger!</strong> <%- danger %>
            </div>
        <% } else if (info.length > 0) { %>
            <div class="alert alert-info alert-dismissible message" style="text-align: center;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Info!</strong> <%- info %>
            </div>
        <% } %>
        <div class="row mx-0">
            <div class="col-md-8 animate-box" data-animate-effect="fadeInLeft">
				<h2 class="post-title" style="text-align: center;"><%- review.title %></h2>
				<p class="singlePostMeta" style="text-align: center;">Posted by
					<a href="#"><%- review.username %></a>
					on <%- review.createdAt.toDateString() %>
                </p>
                <br>
                <%if (review.video) { %>
                    <div class="fh5co_news_img_video"><iframe class="responsive-iframe"
                        src="<%- review.video %>"
                        frameborder="0" allowfullscreen></iframe></div>
                    <br>
                <% } %>
                <%- review.content %>
            </div>
            <div class="col-md-3 animate-box" data-animate-effect="fadeInRight">
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">Tags</div>
                </div>
                <div class="clearfix"></div>
                <div class="fh5co_tags_all">
                    <a href="#" class="fh5co_tagg">Business</a>
                    <a href="#" class="fh5co_tagg">Technology</a>
                    <a href="#" class="fh5co_tagg">Sport</a>
                    <a href="#" class="fh5co_tagg">Art</a>
                    <a href="#" class="fh5co_tagg">Lifestyle</a>
                    <a href="#" class="fh5co_tagg">Three</a>
                    <a href="#" class="fh5co_tagg">Photography</a>
                    <a href="#" class="fh5co_tagg">Lifestyle</a>
                    <a href="#" class="fh5co_tagg">Art</a>
                    <a href="#" class="fh5co_tagg">Education</a>
                    <a href="#" class="fh5co_tagg">Social</a>
                    <a href="#" class="fh5co_tagg">Three</a>
                </div>
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom pt-3 py-2 mb-4">Most Popular</div>
                </div>
                <% for(var i=0; i < Math.min(trending.length, 5); i++) { %>
                    <div class="row pb-3">
                        <div class="col-5 align-self-center">
                            <img src="<%- trending[i].image %>" alt="img" class="fh5co_most_trading"/>
                        </div>
                        <div class="col-7 paddding">
                            <div class="most_fh5co_treding_font">
                                <a href="/post/<%- trending[i].postType %>/<%- trending[i].slug %>/<%- trending[i]._id %>/" class="fh5co_magna py-2"> <%- trending[i].title %></a>
                            </div>
                            <div class="most_fh5co_treding_font_123"> <i class="fa fa-clock-o"></i> <%- trending[i].createdAt.toDateString() %></div>
                        </div>
                    </div>
                <% } %>
            </div>
        </div>
    </div>
</div>

<div class="container-fluid pb-4 pt-5">
    <div class="container animate-box">
        <div>
            <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">Recent Travel Story</div>
        </div>
        <div class="owl-carousel owl-theme" id="slider2">
            <% for(var i=0; i < Math.min(lastModified.length, 6); i++) { %>
                <div class="item px-2">
                    <div class="fh5co_hover_news_img">
                        <div class="fh5co_news_img"><img src="<%- lastModified[i].image %>" alt=""/></div>
                        <div>
                            <a href="/post/<%- lastModified[i].postType %>/<%- lastModified[i].slug %>/<%- lastModified[i]._id %>/" class="d-block fh5co_small_post_heading"><span class=""><%- lastModified[i].title %></span></a>
                            <div class="c_g"><i class="fa fa-clock-o"></i> <%- lastModified[i].createdAt.toDateString() %></div>
                        </div>
                    </div>
                </div>
            <% } %>
        </div>
    </div>
</div>

<div id="wpac-comment"></div>
<script type="text/javascript">
	wpac_init = window.wpac_init || [];
	wpac_init.push({widget: 'Comment', id: 29074});
	(function() {
		if ('WIDGETPACK_LOADED' in window) return;
		WIDGETPACK_LOADED = true;
		var mc = document.createElement('script');
		mc.type = 'text/javascript';
		mc.async = true;
		mc.src = 'https://embed.widgetpack.com/widget.js';
		var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(mc, s.nextSibling);
	})();
</script>

