<%if (auth=='600c0f0fbc4b391f0135065e') { %>
    <div class="editPost">
        <a href="/new-travelPost-<%= stateName %>-<%= locationName %>-<%= locationId %>" ><i class="fa fa-plus"></i></a>
        <a href="/get-location-<%= stateName %>-<%= locationName %>-<%= locationId %>" ><i class="fa fa-pencil-square-o"></i></a>
        <a href="/delete-location-<%= stateName %>-<%= locationName %>-<%= locationId %>" onclick="return confirm('Are you sure you want to delete this post?');"><i class="fa fa-trash"></i></a>
    </div>
    <br><br>
<% } %>
<div class="container-fluid pb-4 pt-4 paddding">
    <div class="container paddding">
        <div class="row mx-0">
            <div class="col-md-8 animate-box" data-animate-effect="fadeInLeft">
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">News</div>
                </div>
                <% for(var i=0; i < travelPost.length; i++) { %>
                    <div class="row pb-4">
                        <div class="col-md-5">
                            <div class="fh5co_hover_news_img">
                                <div class="fh5co_news_img"><img src="<%- travelPost[i].image %>"/></div>
                                <div></div>
                            </div>
                        </div>
                        <div class="col-md-7 animate-box">
                            <a href="/<%= stateName %>-<%= locationName %>-single-travelPost-<%- travelPost[i].slug %>-<%- travelPost[i]._id %>" class="fh5co_magna py-2"> <%- travelPost[i].title %></a><br>
                            <a href="#" class="fh5co_mini_time py-3"> <%- travelPost[i].username %></a>
                            on <%- travelPost[i].createdAt.toDateString() %>
                            <div class="fh5co_consectetur"><%- travelPost[i].description %></div>
                        </div>
                    </div>
                <% } %>
            </div>
            <div class="col-md-3 animate-box" data-animate-effect="fadeInRight">
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">Tags</div>
                </div>
                <div class="clearfix"></div>
                <div class="fh5co_tags_all">
                    <a href="#" class="fh5co_tagg">Business</a>
                    <a href="#" class="fh5co_tagg">Technology</a>
                    <a href="#" class="fh5co_tagg">Sport</a>
                    <a href="#" class="fh5co_tagg">Art</a>
                    <a href="#" class="fh5co_tagg">Lifestyle</a>
                    <a href="#" class="fh5co_tagg">Three</a>
                    <a href="#" class="fh5co_tagg">Photography</a>
                    <a href="#" class="fh5co_tagg">Lifestyle</a>
                    <a href="#" class="fh5co_tagg">Art</a>
                    <a href="#" class="fh5co_tagg">Education</a>
                    <a href="#" class="fh5co_tagg">Social</a>
                    <a href="#" class="fh5co_tagg">Three</a>
                </div>
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom pt-3 py-2 mb-4">Most Popular</div>
                </div>
                <% for(var i=0; i < Math.min(posts.length, 5); i++) { %>
                    <div class="row pb-3">
                        <div class="col-5 align-self-center">
                            <img src="<%- posts[i].image %>" alt="img" class="fh5co_most_trading"/>
                        </div>
                        <div class="col-7 paddding">
                            <div class="most_fh5co_treding_font"> <%- posts[i].title %>.</div>
                            <div class="most_fh5co_treding_font_123"> <%- posts[i].createdAt.toDateString() %></div>
                        </div>
                    </div>
                <% } %>
            </div>
        </div>
        <div class="row mx-0">
            <div class="col-12 text-center pb-4 pt-4">
                <% if (prevPage > 0) { %>
                    <a href="/<%= stateName %>-<%= locationName %>-travelPosts-<%= locationId %>-<%- prevPage %>" class="btn_mange_pagging">Prev</a>
                <% } else { %>
                    <a href="/<%= stateName %>-<%= locationName %>-travelPosts-<%= locationId %>-<%- currPageNo %>" class="btn_mange_pagging">Prev</a>
                <% } %>
                <% if (currPageNo == pageCount) { %>
                    <a href="#" class="btn_pagging">...</a>
                    <a href="/<%= stateName %>-<%= locationName %>-travelPosts-<%= locationId %>-<%- currPageNo %>" class="btn_pagging"><%- currPageNo %></a>
                <% } else { %>
                    <a href="/<%= stateName %>-<%= locationName %>-travelPosts-<%= locationId %>-<%- currPageNo %>" class="btn_pagging"><%- currPageNo %></a>
                    <a href="#" class="btn_pagging">...</a>
                <% } %>
                <% if (nextPage <= pageCount) { %>
                    <a href="/<%= stateName %>-<%= locationName %>-travelPosts-<%= locationId %>-<%- nextPage %>" class="btn_mange_pagging">Next</a>
                <% } else { %>
                    <a href="/<%= stateName %>-<%= locationName %>-travelPosts-<%= locationId %>-<%- currPageNo %>" class="btn_mange_pagging">Next</a>
                <% } %>
             </div>
        </div>
    </div>
</div>

<div class="container-fluid pb-4 pt-5">
    <div class="container animate-box">
        <div>
            <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">News</div>
        </div>
        <div class="owl-carousel owl-theme" id="slider2">
            <% for(var i=0; i < Math.min(posts.length, 6); i++) { %>
                <div class="item px-2">
                    <div class="fh5co_hover_news_img">
                        <div class="fh5co_news_img"><img src="<%- posts[i].image %>" alt=""/></div>
                        <div>
                            <a href="single.html" class="d-block fh5co_small_post_heading"><span class=""><%- posts[i].title %></span></a>
                            <div class="c_g"><i class="fa fa-clock-o"></i> <%- posts[i].createdAt.toDateString() %></div>
                        </div>
                    </div>
                </div>
            <% } %>
        </div>
    </div>
</div>