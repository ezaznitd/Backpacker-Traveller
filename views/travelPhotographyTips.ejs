
<header class="masthead">
	<div class="overlay"></div>
	<div class="container">
		<div class="row">
			<div class="col-lg-8 col-md-10 mx-auto">
				<div class="site-heading">
					<h1>WELCOME</h1>
					<span class="subheading">Travel smarter, cheaper, longer Where do you want to go?</span>
				</div>
			</div>
		</div>
	</div>
</header>

<%if (auth=='600c0f0fbc4b391f0135065e') { %>
    <div class="editPost">
        <a href="/get/travelPhotographyTips/<%= id %>/" ><i class="fa fa-pencil-square-o"></i></a>
    </div>
    <br><br>
<% } %>
<div id="fh5co-single-content" class="container-fluid pb-4 pt-4 paddding">
    <div class="container paddding">
        <%if (success.length > 0) { %>
            <div class="alert alert-success alert-dismissible message" style="text-align: center;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Congratulations!</strong> <%- success %>
            </div>
        <% } else if (warning.length > 0) { %>
            <div class="alert alert-warning alert-dismissible message" style="text-align: center;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Warning!</strong> <%- warning %>
            </div>
        <% } else if (info.length > 0) { %>
            <div class="alert alert-info alert-dismissible message" style="text-align: center;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Info!</strong> <%- info %>
            </div>
        <% } %>
        <div class="row mx-0">
            <div class="col-md-8 animate-box" data-animate-effect="fadeInLeft">
				<h2 class="post-title" ><%- travelPhotographyTips.title %></h2>
				<p class="singlePostMeta">Posted by
					<a href="#"><%- travelPhotographyTips.username %></a>
					on <%- travelPhotographyTips.createdAt.toDateString() %>
                </p>
				<p class="post-description" ><%- travelPhotographyTips.description %></p>
				<br>
				<%- travelPhotographyTips.content %>
				<br>
            </div>
            <div class="col-md-3 animate-box" data-animate-effect="fadeInRight">
                <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">Tags</div>
                </div>
                <div class="clearfix"></div>
                <div class="fh5co_tags_all">
                    <a href="#" class="fh5co_tagg">Business</a>
                    <a href="#" class="fh5co_tagg">Technology</a>
                    <a href="#" class="fh5co_tagg">Sport</a>
                    <a href="#" class="fh5co_tagg">Art</a>
                    <a href="#" class="fh5co_tagg">Lifestyle</a>
                    <a href="#" class="fh5co_tagg">Three</a>
                    <a href="#" class="fh5co_tagg">Photography</a>
                    <a href="#" class="fh5co_tagg">Lifestyle</a>
                    <a href="#" class="fh5co_tagg">Art</a>
                    <a href="#" class="fh5co_tagg">Education</a>
                    <a href="#" class="fh5co_tagg">Social</a>
                    <a href="#" class="fh5co_tagg">Three</a>
                </div>
                <!-- <div>
                    <div class="fh5co_heading fh5co_heading_border_bottom pt-3 py-2 mb-4">Most Popular</div>
                </div> -->
            </div>
        </div>
    </div>
</div>

<div id="wpac-comment"></div>
