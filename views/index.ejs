
<div class="container-fluid pt-3">
    <div class="container animate-box" data-animate-effect="fadeIn">
        <%if (success.length > 0) { %>
            <div class="alert alert-success alert-dismissible message" style="text-align: center;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Congratulations!</strong> <%- success %>
            </div>
        <% } else if (warning.length > 0) { %>
            <div class="alert alert-warning alert-dismissible message" style="text-align: center;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Warning!</strong> <%- warning %>
            </div>
        <% } else if (info.length > 0) { %>
            <div class="alert alert-info alert-dismissible message" style="text-align: center;">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>Info!</strong> <%- info %>
            </div>
        <% } %>
        <div>
            <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4">Trending</div>
        </div>
        <div class="owl-carousel owl-theme js" id="slider1">
            <% for(var i=0; i < Math.min(trending.length, 6); i++) { %>
                <div class="item px-2">
                    <div class="fh5co_latest_trading_img_position_relative">
                        <div class="fh5co_latest_trading_img"><img src="<%- trending[i].image %>" alt=""
                                                            class="fh5co_img_special_relative"/></div>
                        <div class="fh5co_latest_trading_img_position_absolute"></div>
                        <div class="fh5co_latest_trading_img_position_absolute_1">
                            <a href="/single-post-<%- trending[i].postType %>-<%- trending[i].slug %>-<%- trending[i]._id %>" class="text-white"> <%- trending[i].title %> </a>
                            <div class="fh5co_latest_trading_date_and_name_color"> <%- trending[i].username %> on <i class="fa fa-clock-o"></i> <%- trending[i].createdAt.toDateString() %> </div>
                        </div>
                    </div>
                </div>
            <% } %>
        </div>
    </div>
</div>

<div class="container-fluid pb-4 pt-5">
    <div class="container animate-box">
        <div>
            <div class="fh5co_heading fh5co_heading_border_bottom py-2 mb-4"> Adventure Stories</div>
        </div>
        <div class="owl-carousel owl-theme" id="slider2">
            <% for(var i=0; i < Math.min(travelStories.length, 6); i++) { %>
                <div class="item px-2">
                    <div class="fh5co_hover_news_img">
                        <div class="fh5co_news_img"><img src="<%- travelStories[i].image %>" alt=""/></div>
                        <div>
                            <a href="/single-post-<%- travelStories[i].postType %>-<%- travelStories[i].slug %>-<%- travelStories[i]._id %>" class="d-block fh5co_small_post_heading"><span class=""><%- travelStories[i].title %></span></a>
                            <div class="c_g"><i class="fa fa-clock-o"></i> <%- travelStories[i].createdAt.toDateString() %></div>
                        </div>
                    </div>
                </div>
            <% } %>
        </div>
    </div>
</div>
